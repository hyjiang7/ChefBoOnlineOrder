ate({status: false, error:\"\"});\n  const router = useRouter();\n\n  const handleModChange = (modIdAdd, modIdRemove, price, name, reqMod) => {\n    let removeIndex;\n    let selectedModsTemp = selectedMods;\n    setReqModSelected({...selectedMods, status:reqMod})\n\n    var modObj = {};\n    modObj.ID = modIdAdd;\n    modObj.Price = price;\n    modObj.Name = name;\n\n\n    if (modIdRemove) {\n      removeIndex = selectedModsTemp.findIndex((i) => {\n        return i.ID == modIdRemove.toString();\n      });\n      selectedModsTemp.splice(removeIndex, 1);\n    }\n\n    if (modIdAdd) {\n      selectedModsTemp.push(modObj);\n    }\n\n    setSelectedMods(selectedModsTemp);\n    setItemTotal(price + itemTotal);\n  };\n\n  const handleAddToCart = (e) => {\n    \n    console.log(reqModSelected.status)\n    e.preventDefault();\n\n    if(reqModSelected.status){\n        const sendItem = {\n            item: item.detail,\n            mods: selectedMods,\n        };\n    \n        const { cart } = store.getState();\n    \n        store.dispatch(addToCart(sendItem, cart));\n        router.push(\"/routes/pickup\")\n    }\n    else{\n\n        setReqModSelected({...selectedMods, error:\"Please make a selection\"})\n    }\n\n  };\n\n  return (\n    <div className=\"p-2 px-3 col col-md-8 col-lg-6  m-auto justify-content-center\">\n      <Link href=\"/routes/pickup\">\n        <i\n          className={`${utility.fontSizeMedium} far fa-arrow-circle-left `}\n        ></i>\n      </Link>\n      <div>\n        <div className={`${utility.fontSizeMedium}`}>\n          <MenuItemName name={Name} />\n          <MenuItemDes des={Description} />\n          <MenuItemPrice price={Price} />\n        </div>\n        <div>\n          <form>\n            {item.RequiredMods <= 0 ? null : (\n                <div>\n                        <p>Required Selection</p>\n                        {reqModSelected.error ==\"\"? <p>{reqModSelected.error}</p>:null}\n                        <RequiredMods\n                            mods={item.RequiredMods}\n                            onchange={handleModChange}\n                        />\n                </div>\n                \n            )}\n\n            {item.OptionalMods <= 0 ? null : (\n              <OptionalMods\n                mods={item.OptionalMods}\n                onchange={handleModChange}\n              />\n            )}\n\n            <div className=\" px-3 row justify-content-center\">\n              <Button\n                variant=\"dark\"\n                className=\"col col-md-6\"\n                type=\"submit\"\n                onClick={handleAddToCart}\n              >\n                Add to Cart $ {itemTotal.toFixed(2)}\n              </Button>\n            </div>\n          </form>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport async function getServerSideProps({ params: { id } }) {\n  const res = await fetch(`http://localhost:5001/details/${id}`);\n  const item = await res.json();\n\n  // server side rendering\n  return {\n    props: { item }, // will be passed to the page component as props\n  };\n}\n\nexport default itemDetail;\n"]},"metadata":{},"sourceType":"module"}